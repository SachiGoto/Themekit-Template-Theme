<div class ="gallery">

  {% for block in section.blocks %}
    <div class="image-wrap" style="background-image:url('{{ block.settings.image | img_url}}');">
    
    </div>
    {% endfor %}


</div>



{% schema %}
  {
    "name": "Gallery",
    "settings": [],
    "blocks":[
      {
        "type":"image",
        "name": "Image Block",
         "settings":[
           {
             "type":"image_picker",
             "id":"image",
             "label":"Image"
           }
         ]
      }
    ]
  }
{% endschema %}

{% stylesheet %}
  .gallery{
   background-color:#FFC0CB;
  }
      .image-wrap{
        display: inline-block;
         width:30%;
         background-size:cover;
      }
{% endstylesheet %}

